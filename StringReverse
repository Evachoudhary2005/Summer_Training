#include<iostream>
#include<string.h>
using namespace std;
int main() {
    string a = "Pneumonoultramicroscopicsilicovolcanoconiosis";
    cout << a << endl;
    for(int i = a.size() - 1; i >= 0; i--) {
        cout << a[i] << endl;
    }
    for(int i = a.size() - 1; i >= 0; i--) {
        cout << a[i];
    }
    cout << endl;
    return 0;
}

#include<iostream>
using namespace std;
int main(){
    int arr[5]={10,20,30,40,50};
    int *p = arr;
    cout<<"Array elements using pointer: ";
    for (int i=0;i<5;i++){
        cout<< *(p+i)<<" ";  
    }
    cout<<endl;
    cout<<"Array elements using pointer with array name: ";
    for(int i=0;i<5;i++) {
        cout<< *(arr+i)<<" ";
    }
    return 0;
}

#include<iostream>
using namespace std;
struct blinkit{
    int cityId;
    string name;
    string city;

};
int main(){
    blinkit b[5];
    b[0].cityId=1;
    b[0].name="Eva";
    b[0].city="Mumbai";

    b[1].cityId=2;
    b[1].name="Harshdeep";
    b[1].city="Delhi";
    
    b[2].cityId=3;
    b[2].name="Jasleen";
    b[2].city="Banglore";
    
    b[3].cityId=4;
    b[3].name="Arpita";
    b[3].city="Kolkata";
    
    b[4].cityId=5;
    b[4].name="Lily";
    b[4].city="Chennai";
    
    cout<<"Blinkit Members in different cities are: "<<endl;
    for(int i=0;i<5;i++ ){
        cout<<"ID: "<<b[i].cityId<<endl;
        cout<<"Name: "<<b[i].name<<endl;
        cout<<"City: "<<b[i].city<<endl;
    }
}

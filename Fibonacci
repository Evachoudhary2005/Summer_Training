#include<iostream>
using namespace std;
void printFib(int n){
    int a=0,b=1, c;
    if (n==0){
        cout<<a;
    }
    else if(n==1){
        cout<<a<<","<<b;
    }
    else {
        cout<<a<<","<<b;
        for (int i=2;i<n;i++){
            c=a+b;
            cout<<","<<c;
            a=b;
            b=c;
        }
    }
        cout<<endl;
}
    int main() {
        int n;
        cout<< "Enter the number of terms in the Fibonacci Series:- ";
        cin>>n;
        printFib(n);
        return 0;
}

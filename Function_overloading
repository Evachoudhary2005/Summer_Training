#include<iostream>
using namespace std;
void Math(){
    int a,b;
    cin>>a>>b;
    cout<<"Addition of numbers a and b is: "<<a+b;
}
void Math(int a, int b){
    cout<<a<<b<<endl;
    cout<<"Multiplacation of two numbers a and b is: "<<a*b;
}
void Math(int a,int b,int c){
    cout<<a<<b<<c;
    cout<<"Subtraction of two numbers a and b is: "<<(a-b)-c<<endl;
}
int main(){
    Math();
    Math(3,7);
    Math(4,5,0);
}

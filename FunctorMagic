#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
class sum
{
    private:
    int v;
    public:
    sum(int x):v(x){}
    int operator()(int y){
        return y+=v;
    }
};
int main(){
    vector<int> num={1,2,3,4,5,6,7,8,9};
    sum s(10);
    for(int &a: num){
        s(a);
    }
    for(int a:num){
        cout<<a<<" ";
    }
}

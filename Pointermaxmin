#include<iostream>
using namespace std;
int main() {
    int a=10;
    int b=20;
    int *p= &a;
    int *q= &b;
    if(*p > *q){
        cout <<"Maximum Number is: "<<a<<endl;
        cout<< "Minimum Number is: "<<b;
    }
    else if(*p < *q) {
        cout<<"Maximum Number is: "<<b<<endl;
        cout<<"Minimum Number is: "<<a;
    }
    else {
        cout<<"Both are equal.";
    }
    return 0;
}

#include <iostream>
using namespace std;
int main() {
    int a[] = {10, 20, 10, 5, 20, 30, 5};
    int n = sizeof(a) / sizeof(a[0]);
    int unique[n];  
    int count = 0; 
    for (int i = 0; i < n; i++) {
        bool found = false;
        for (int j = 0; j < count; j++) {
            if (a[i] == unique[j]) {
                found = true;
                break;
            }
        }
        if (!found) {
            unique[count] = a[i];
            count++;
        }
    }
    cout << "After removing duplicates: ";
    for (int i = 0; i < count; i++) {
        cout << unique[i] << " ";
    }
    return 0;
}

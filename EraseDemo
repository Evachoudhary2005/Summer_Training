//erase code
#include<iostream>
#include<algorithm>
#include<vector>
using namespace std;
int main() {
    vector<int> v1={1,2,3,4};
    vector<int> v2={5,6,7,8};
    vector<int> v3(8);
    cout<<"Before Erasing: "<<endl;
    for(int n:v1){
    cout<<n<<" ";
    }
    cout<<endl;
    for(int n:v2){
        cout<<n<<" ";
    }
    cout<<endl;
    v1.erase(remove(v1.begin(),v1.end(),2));
    v2.erase(remove(v2.begin(),v2.end(),5));
    cout<<"After Erasing: "<<endl;
    for(int n:v1){
        cout<<n<<" ";
    }
    cout<<endl;
    for(int n:v2){
        cout<<n<<" ";
    }
}
